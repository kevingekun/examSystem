<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wondersgroup.falcon.model.select">

	<class name="Record" table="RECORDORIGINALDATA">
		<id name="recordreference" column="recordreference" type="java.lang.String">
			<generator class="assigned"/>
		</id>
		
		<property name="startrecordtime" type="string" length="255"/>
		<property name="stoprecordtime" type="string" length="255"/>
		<property name="agentid" type="string" length="255"/>
		<property name="callerid" type="string" length="255"/>
		<property name="flag" type="string" length="255"/>
		
		
	</class>
	
	
</hibernate-mapping>