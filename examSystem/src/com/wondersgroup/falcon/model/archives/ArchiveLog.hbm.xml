<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wondersgroup.falcon.model.archives">

	<class name="ArchiveLog">
		<id name="id">
			<generator class="native">
				<param name="sequence">archive_log_sequence</param>
			</generator>
		</id>
		<property name="archiveid" not-null="true" />
		<property name="callid" not-null="true" />
		<property name="userid" not-null="true" />
		<property name="dt" not-null="true" />
		<property name="type" not-null="true" />
	</class>
	 
</hibernate-mapping>
