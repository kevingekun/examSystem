<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wondersgroup.falcon.model.citizeninfo">

	<class name="HisNode" table="HISNODE">
		<id name="id" column="id" type="java.lang.Long">   
			<generator class="native">         
                <param name="sequence">TEMP5</param>
            </generator>
		</id>
		<property name="parent_id" type="int" />
		<property name="ordering" type="int" />
		<property name="name" type="string" />
		<property name="visible" type="int" />
		<property name="paixu" type="java.lang.Long" />
	</class>
	
	
</hibernate-mapping>