<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wondersgroup.popedom.bo">

	<class name="EuserTest" table="E_USERTEST">
		<id name="id" >
			<generator class="native">
				<param name="sequence">user_id_sequence</param>
			</generator>
		</id>
		<property name="provenance" type="string" />
		<property name="referencenum" type="string" />
		<property name="businesstype" type="string"  />
		<property name="servicetype" type="string"/>
		<property name="difficultylevel" type="string"/>
		<property name="questiontype" type="string"/>
		<property name="time" type="date"/>
		<property name="questionprop" type="string"/>
		<property name="topic" type="string"/>
		<property name="options" type="string"/>
		<property name="answer" type="string"/>
	</class>
</hibernate-mapping>