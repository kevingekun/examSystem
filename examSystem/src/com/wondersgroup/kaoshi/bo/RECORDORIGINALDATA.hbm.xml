<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wondersgroup.kaoshi.bo">

	<class name="RECORDORIGINALDATA" table="RECORDORIGINALDATA1">
		<id name="recordreference" type="string">
			<generator class="uuid">
			</generator>
		</id>
		<property name="agentid" type="string"/>
		<property name="startrecordtime" />	
		<property name="callerid" type="string"/>
		<property name="recordlength" type="java.lang.Long"/>
		<property name="flag" type="string"/>
		<property name="directionflag" type="string"/>
		<property name="reservedthree" type="string"/>
		<property name="voiceip" column="VOICEIP" type="string"/>
		<property name="voiceid" column="VOICEID" type="string"/>
		<property name="channel" column="CHANNEL" type="string"/>
	</class>
	
</hibernate-mapping>